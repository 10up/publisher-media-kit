{
  "name": "publisher-media-kit",
  "version": "1.3.0",
  "scripts": {
    "start": "10up-toolkit start",
    "watch": "10up-toolkit watch",
    "build": "10up-toolkit build",
    "format-js": "10up-toolkit format-js",
    "lint-js": "10up-toolkit lint-js",
    "lint-style": "10up-toolkit lint-style",
    "test": "10up-toolkit test-unit-jest",
    "wp-env": "wp-env",
    "cypress:open": "CYPRESS_BASE_URL=http://localhost:8889/ cypress open --config-file tests/cypress/config.config.js",
    "cypress:run": "CYPRESS_BASE_URL=http://localhost:8889/ cypress run --config-file tests/cypress/config.config.js",
    "clean-dist": "rm -rf ./dist"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "10up-toolkit": "^4.0.0",
    "@10up/babel-preset-default": "^1.1.2",
    "@10up/eslint-config": "^2.4.6",
    "@babel/preset-react": "^7.17.12",
    "@wordpress/env": "^5.2.0",
    "cypress": "^11.2.0",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^8.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^37.0.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^2.5.0"
  },
  "dependencies": {
    "@10up/component-tabs": "^2.0.1",
    "@wordpress/blocks": "^11.9.0",
    "@wordpress/hooks": "^3.10.0",
    "@wordpress/i18n": "^4.10.0",
    "@wordpress/icons": "^2.9.0",
    "classnames": "^2.3.1",
    "normalize.css": "^8.0.1",
    "path": "^0.12.7",
    "prop-types": "^15.8.1",
    "react-autocomplete": "^1.8.1",
    "react-sortable-hoc": "^2.0.0"
  },
  "10up-toolkit": {
    "devURL": "https://publisher-media.test/",
    "entry": {
      "admin": "./assets/js/admin/admin.js",
      "frontend": "./assets/js/frontend/frontend.js",
      "shared": "./assets/js/shared/shared.js",
      "admin-style": "./assets/css/admin/admin-style.css",
      "shared-style": "./assets/css/shared/shared-style.css",
      "style": "./assets/css/frontend/style.css",
      "tabs-block": "./includes/blocks/block-editor/tabs/register.js",
      "tabs-item-block": "./includes/blocks/block-editor/tabs-item/register.js"
    }
  }
